{
  "multipart": [
    {
      "apply": {
        "model": "ae2:block/controller/controller_block_offline"
      },
      "when": {
        "state": "offline",
        "type": "block"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_block_online"
      },
      "when": {
        "state": "online",
        "type": "block"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_block_conflicted"
      },
      "when": {
        "state": "conflicted",
        "type": "block"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_offline",
        "x": 90,
        "y": 90
      },
      "when": {
        "state": "offline",
        "type": "column_x"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_offline"
      },
      "when": {
        "state": "offline",
        "type": "column_y"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_offline",
        "x": 90
      },
      "when": {
        "state": "offline",
        "type": "column_z"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_online",
        "x": 90,
        "y": 90
      },
      "when": {
        "state": "online",
        "type": "column_x"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_online"
      },
      "when": {
        "state": "online",
        "type": "column_y"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_online",
        "x": 90
      },
      "when": {
        "state": "online",
        "type": "column_z"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_conflicted",
        "x": 90,
        "y": 90
      },
      "when": {
        "state": "conflicted",
        "type": "column_x"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_conflicted"
      },
      "when": {
        "state": "conflicted",
        "type": "column_y"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_column_conflicted",
        "x": 90
      },
      "when": {
        "state": "conflicted",
        "type": "column_z"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_inside_a"
      },
      "when": {
        "state": "offline|online",
        "type": "inside_a"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_inside_b"
      },
      "when": {
        "state": "offline|online",
        "type": "inside_b"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_inside_a_conflicted"
      },
      "when": {
        "state": "conflicted",
        "type": "inside_a"
      }
    },
    {
      "apply": {
        "model": "ae2:block/controller/controller_inside_b_conflicted"
      },
      "when": {
        "state": "conflicted",
        "type": "inside_b"
      }
    }
  ]
}
