{
  "multipart": [
    {
      "apply": {
        "model": "ae2:block/wireless_access_point_chassis",
        "y": 180
      }
    },
    {
      "when": {
        "state": "off"
      },
      "apply": {
        "model": "ae2:block/wireless_access_point_status_off",
        "y": 180
      }
    },
    {
      "when": {
        "state": "off|on"
      },
      "apply": {
        "model": "ae2:block/wireless_access_point_off",
        "y": 180
      }
    },
    {
      "when": {
        "state": "on"
      },
      "apply": {
        "model": "ae2:block/wireless_access_point_status_on",
        "y": 180
      }
    },
    {
      "when": {
        "state": "has_channel"
      },
      "apply": {
        "model": "ae2:block/wireless_access_point_on",
        "y": 180
      }
    },
    {
      "when": {
        "state": "has_channel"
      },
      "apply": {
        "model": "ae2:block/wireless_access_point_status_has_channel",
        "y": 180
      }
    }
  ]
}
